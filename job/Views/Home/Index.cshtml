@section Scripts{
    <script>
        createVueApp({
            /*返回job数据*/
            data() {
                return {
                    jobs: [],
                    newslist: [],
                    pagination: {
                        pageIndex: 1,
                        pageSize: 3,
                        totalCount: 0,
                    },
                    iptTitle: '',
                    iptCategory: '',
                    iptUpdatetime: '',
                    searchInput: '',
                    addrInput: '',
                    interviewDateTime1: '',
                    interviewDateTime2: '',
                    interviewDateTime3: '',
                    interviewOther: '',
                    jobId: ''
                }
            },
            /*页面加载完成后回调下方的工作列表方法*/
            mounted() {
                this.handleGetjobs();
                this.handleGetNews();
            },
            /*获取列表*/
            methods: {
                // 文字长度大于100，显示省略号
                showEllipsis(text) {
                    if (text == null) text = ""
                    let len = text.length;
                    //if (document.body.clientWidth <= 992 && len > 100) {
                    //    text = text.substr(0, 100).concat('...');
                    //}
                    text = text.substr(0, 100).concat('...');
                    return text;
                },
                handleGetjobs() {
                    this.getjobs(this.pagination).then(res => {
                        this.jobs = res.items
                        this.pagination.totalCount = res.totalCount
                    })
                },
                handleGetNews() {
                    var pagination = {
                        pageIndex: 1,
                        pageSize: 3
                    }
                    this.getNews(pagination).then(res => {
                        this.newslist = res.items
                    })
                },
                handleTabClick() {
                },
                /*发送请求*/
                getjobs(pagination) {
                    //return this.$http.post('/job/getlist', pagination)
                    return this.$http.post('/job/getlist', pagination) // check the route
                },
                getNews(pagination) {
                    //return this.$http.post('/job/getlist', pagination)
                    return this.$http.post('/news/getlist', pagination) // check the route
                },
                getSearch() {
                    //this.pagination.Title = this.searchInput
                    //this.pagination.Workplace = this.addrInput
                    //this.getjobs(this.pagination).then(res => {
                    //    this.jobs = res.items
                    //    this.pagination.totalCount = res.totalCount
                    //})
                    var url = "/job/JobList?Title=" + encodeURI(this.searchInput) + "&Workplace=" + encodeURI(this.addrInput)
                    window.open(url);
                },
                getImg(item, index) {
                    return index === this.tabIndex
                        ? "./imgs/" + item.img2
                        : "./imgs/" + item.img1;
                },
                changeTab(index) {
                    this.tabIndex = index;
                },
                handleClickBigArea(item) {
                    console.log(item);
                    this.selectCity = item.city;
                },
                backAreaList() {
                    this.selectCity = "";
                },
                getJobId(id) {
                    this.jobId = id;
                },
                handleGetContents() {
                    var data = {
                        JobID: this.jobId,
                        InterviewDateTime1: this.datePipe(this.interviewDateTime1),
                        InterviewDateTime2: this.datePipe(this.interviewDateTime2),
                        InterviewDateTime3: this.datePipe(this.interviewDateTime3),
                        Other: this.interviewOther
                    }
                    this.AddInterview(data).then(res => {
                        
                    })

                },
                AddInterview(data) {
                    return this.$http.post('/Job/AddInterview', data)
                },
                datePipe(timestamp, format = 'yyyy-MM-dd hh:mm:ss') {
                    let date = new Date(timestamp);
                    var o = {
                        'y+': date.getFullYear(),
                        'M+': date.getMonth() + 1, // 月份 "d+": value.getDate(), // 日
                        'd+': date.getDate(),
                        'h+': date.getHours(), // 小时 "m+": value.getMinutes(), // 分 "s+": value.getSeconds(), // 秒
                        'm+': date.getMinutes(),
                        's+': date.getSeconds()
                    };
                    if (/(y+)/.test(format)) {
                        format = format.replace(RegExp.$1, (date.getFullYear() + '').substr(4 - RegExp.$1.length));
                    }
                    for (var k in o) {
                        if (new RegExp('(' + k + ')').test(format)) {
                            format = format.replace(
                                RegExp.$1,
                                RegExp.$1.length == 1 ? o[k] : ('00' + o[k]).substr(('' + o[k]).length)
                            );
                        }
                    }
                    return format;
                }
            }
        })
        $('#header').hide()
    </script>


